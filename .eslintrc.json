{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "camelcase": 0,
        "camel_case": 1,
        "quotes": 0,
        "no-unused-vars": 0,
        "no-underscore-dangle": 0,
        "comma-dangle": 0
            }
}
